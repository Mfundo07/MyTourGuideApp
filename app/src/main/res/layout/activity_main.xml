<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.android.mytourguideapp.MainActivity">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Edit View Data"
        android:layout_margin="16dp"
        android:gravity="center"
        android:id="@+id/view_data"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Edit Eat data"
        android:layout_margin="16dp"
        android:gravity="center"
        android:id="@+id/eat_data"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Edit Drink Data"
        android:layout_margin="16dp"
        android:layout_gravity="center"
        android:id="@+id/drink_data"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Edit Stay Data"
        android:layout_margin="16dp"
        android:gravity="center"
        android:id="@+id/stay_data"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tour guide"
        android:layout_margin="16dp"
        android:gravity="center"
        android:id="@+id/tour_guide"/>

</LinearLayout>
